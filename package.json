{
  "name": "create-dom-tree",
  "version": "0.1.0",
  "description": "Create DOM trees using hyperscript like syntax",
  "main": "lib/index.js",
  "scripts": {
    "example": "budo ./example/index.js --live --open --dir ./example",
    "lint": "standard \"src/*.js\" \"test/*.js\"",
    "prepublish": "npm run lint && npm run test && mkdirp lib && buble src -o lib",
    "release": "xyz",
    "test": "tape test/*.js | tap-spec"
  },
  "keywords": [
    "hyperscript",
    "dom",
    "create dom",
    "morphdom",
    "virtual-dom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/queckezz/create-dom-tree"
  },
  "author": "queckezz <fabian.eichenberger@gmail.com>",
  "dependencies": {
    "@f/dom-events": "^1.0.4",
    "hyperscript-helpers": "^2.1.1"
  },
  "devDependencies": {
    "buble": "^0.9.3",
    "budo": "^8.2.2",
    "jsdom": "^9.2.0",
    "mkdirp": "^0.5.1",
    "morphdom": "^1.3.1",
    "standard": "^7.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tsml": "^1.0.1",
    "xyz": "^0.5.0"
  },
  "license": "MIT"
}
